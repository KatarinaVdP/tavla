        "format:check": "prettier . --check",
        "lint": "npm run eslint && npm run stylelint && npm run format:check",
        "start": "webpack serve",
        "local": "cd functions && npm run build && cd .. && webpack serve --env development",
        "clean": "rm -rf dist/",
        "build:prod": "npm run clean && webpack --mode production --env prod",
        "build:staging": "npm run clean && webpack --mode production --env staging",
        "serve:prod": "npm run build:prod && firebase serve",
        "serve:staging": "npm run build:staging && firebase serve",
        "deploy:prod": "npm run build:prod && firebase deploy -P prod",
        "deploy:staging": "npm run build:staging && firebase deploy -P staging"
